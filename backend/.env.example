# .env.example

# 데이터베이스 연결 정보
DB_HOST=localhost
DB_USER= # 여기에 실제 사용자 이름 입력
DB_PASSWORD= # 여기에 실제 비밀번호 입력 (비밀번호 관리자에서 공유받으세요)
DB_DATABASE=dream_interpreter_db # 사용할 데이터베이스 이름

# Prisma에서 사용할 데이터베이스 URL (위 정보를 바탕으로 자동 생성될 수 있음)
# DATABASE_URL="mysql://USER:PASSWORD@HOST:PORT/DATABASE" 형식에 맞게 직접 입력하거나,
# Prisma 마이그레이션 시 자동으로 생성되도록 비워둘 수 있습니다.
DATABASE_URL=

# 서버 포트
PORT=3001

# JWT 서명 비밀키
JWT_SECRET= # 여기에 실제 JWT 시크릿 입력 (비밀번호 관리자에서 공유받으세요)

# OpenAI API 키
OPENAI_API_KEY= # 여기에 실제 OpenAI API 키 입력 (비밀번호 관리자에서 공유받으세요)
